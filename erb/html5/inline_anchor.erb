<%#encoding:UTF-8%><%
case @type
when :xref %><%= %|<a href="##@target">#{@text || @document.references[:ids].fetch(@target, "[#{@target}]").tr_s("\n", ' ')}</a>| %><%
when :ref %><%= %|<a id="#@target"></a>| %><%
when :bibref %><%= %|<a id="#@target">[#@target]</a>| %><%
else %><%= %|<a href="#@target"#{attr?('window') ? %( target="#{attr 'window'}") : nil}>#@text</a>| %><%
end %>
